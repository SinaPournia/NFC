<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.parkkolay.nfc.View.StartActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:orientation="vertical">
        <Button
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:text="GEt Finished Fields"
            android:layout_height="wrap_content"
            android:onClick="getFinishidFields"/>
        <Button
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:text="GEt UnFinished Fields"
            android:layout_height="wrap_content"
            android:onClick="getUnFinishidFields"/>

        <Button
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Addtarife"
            android:onClick="addTarife"/>
        <Button
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:text="Debug"
            android:layout_height="wrap_content"
            android:onClick="getRAdioGroup"/>
        <Button
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:text="AddToDataBase"
            android:layout_height="wrap_content"
            android:onClick="AddToDataBase"/>
        <Button
            android:layout_margin="2dp"
            android:layout_width="match_parent"
            android:text="ADDService"
            android:layout_height="wrap_content"
            android:onClick="ADDService"/>
        <EditText
            android:layout_margin="5dp"
            android:id="@+id/CarID"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_margin="5dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:id="@+id/CardIDTextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <RadioGroup
        android:id="@+id/TarifeRadioGroup"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_height="wrap_content" >







    </RadioGroup>

</LinearLayout>
